{
  "name": "ChatGPT Linker",
  "description": "A Chrome extension to connect two instances of ChatGPT.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Open ChatGPT Linker",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": ["activeTab"]
}
